Display a list of cars

<table>
	<thead>
		
	</thead>
	<tbody>
		{{#each model}}
			<tr>
				<td>
					{{#link-to "results.car" this}}{{id}}{{/link-to}}
				</td>
				<td>{{owner.id}} | {{owner.email}}</td>
				<td>
					{{#each passengers}}
						{{id}}|{{email}}
						<br>
					{{/each}}
				</td>
			</tr>
		{{else}}
			<tr>
				<td colspan="20">Sorry, no cars at the moment</td>
			</tr>
		{{/each}}
	</tbody>
</table>
